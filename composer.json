{
    "name": "extractr-io/rialto",
    "description": "Manage Node resources from PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Johann Pardanaud",
            "email": "pardanaud.j@gmail.com"
        }
    ],
    "require": {
        "clue/socket-raw": "^1.2",
        "symfony/process": "^3.3|^4.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.5.3",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "ExtractrIo\\Rialto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExtractrIo\\Rialto\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": "npm install",
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
